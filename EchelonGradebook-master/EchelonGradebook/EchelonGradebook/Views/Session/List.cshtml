@using EchelonGradebook.Models
@model IEnumerable<Login>
@{ 
    ViewData["Title"] = "Login List";
}
<h1>Login List</h1>
<table>
<tr>
    <th>ID</th>
    <th>Username</th>
    <th>Password</th>
</tr>
@foreach(var l in Model)
{
    <tr>@l.ID</tr>
    <tr>@l.username</tr>
    <tr>@l.password</tr>

}
</table>